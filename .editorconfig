root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
charset = utf-8
trim_trailing_whitespace = true

[*.rs]
indent_size = 4

[*.js]
indent_size = 2

[Cargo.{toml,lock}]
indent_size = 1

[.travis.yml]
indent_size = 2

[*.md]
insert_final_newline = false
trim_trailing_whitespace = false
indent_size = 2
